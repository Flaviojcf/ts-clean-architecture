{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": true,
        "decorators": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "target": "es2020",
      "keepClassNames": true
    },
    "module": {
      "type": "commonjs",
  
      "noInterop": true
    }
  }